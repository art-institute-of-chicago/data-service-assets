{
    "name": "aic/data-service-template",
    "description": "A data service for the AIC data hub.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/art-institute-of-chicago/data-hub-foundation.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "aic/data-hub-foundation": "dev-laravel-8-support",
        "aws/aws-sdk-php": "^3.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.5",
        "league/csv": "^9.8",
        "marijnvdwerf/material-palette": "^1.2",
        "marktopper/doctrine-dbal-timestamp-type": "^1.0",
        "sentry/sentry-laravel": "^2.9"
    },
    "require-dev": {
        "brianium/paratest": "^6.4",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.5",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "netresearch/composer-patches-plugin": true
        }
    }
}
